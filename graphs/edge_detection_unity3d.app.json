{
    "name": "edge_detection_unity3d",
    "modules": [
        "sight",
        "viewers"
    ],
    "graph": {
        "nodes": [
            {
                "name": "simulation",
                "subgraph": "packages/navsim/apps/navsim_navigation.subgraph.json"
            },
            {
                "name": "edge_detector",
                "components": [
                    {
                        "name": "ledger",
                        "type": "isaac::alice::MessageLedger"
                    },
                    {
                        "name": "detector",
                        "type": "isaac::alice::PyCodelet"
                    }
                ]
            },
            {
                "name": "detection_viewer",
                "components": [
                    {
                        "name": "ledger",
                        "type": "isaac::alice::MessageLedger"
                    },
                    {
                        "name": "viewer",
                        "type": "isaac::viewers::ImageViewer"
                    }
                ]
            },
            {
                "name": "scene_viewer",
                "components": [
                    {
                        "name": "ledger",
                        "type": "isaac::alice::MessageLedger"
                    },
                    {
                        "name": "viewer",
                        "type": "isaac::viewers::ImageViewer"
                    }
                ]
            }
        ],
        "edges": [
            {
                "source": "simulation.interface/output/color",
                "target": "edge_detector/detector/input_image"
            },
            {
                "source": "edge_detector/detector/output_image",
                "target": "detection_viewer/viewer/image"
            },
            {
                "source": "simulation.interface/output/color",
                "target": "scene_viewer/viewer/image"
            }
        ]
    },
    "config": {
        "detection_viewer": {
            "viewer": {
                "target_fps": 20,
                "reduce_scale": 4
            }
        },
        "websight": {
            "WebsightServer": {
                "port": 3000,
                "ui_config": {
                    "windows": {
                        "Edge Detection": {
                            "renderer": "2d",
                            "dims": {
                                "width": 640,
                                "height": 480
                            },
                            "channels": [
                                {
                                    "name": "edge_detection_unity3d/detection_viewer/viewer/image"
                                }
                            ]
                        },
                        "Scene": {
                            "renderer": "2d",
                            "dims": {
                                "width": 640,
                                "height": 480
                            },
                            "channels": [
                                {
                                    "name": "edge_detection_unity3d/scene_viewer/viewer/image"
                                }
                            ]
                        }
                    }
                }
            }
        }
    }
}